from pandas import *
import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
def manual(dataframe,columnvalues):
   return dataframe.groupby(columnvalues)
def random(dataframe,numvaluestoselect):
   rows = np.random.choice(dataframe.index.values,numvaluestoselect)
   sampled_df = dataframe.ix[rows]
   return sampled_df

